<!--pages/pagelinks/pagelinks.wxml-->

<view class="head_style">
  <image class="logo_style" src='{{src_icon}}'></image>
  <view class="title_style">{{page_title}}</view>
</view>

<view style='display:block'>
  <text class="title1">From Universities</text>
</view>

