    <navigator url="../pagelinks/viewlink/unilinks/viewlinkUni1">
      <image style="width:100%;height:200px;" src="../../img/JobUni1.jpg"></image>
    </navigator>
  </view>
  <view class="view_item" style="width:100%;height:200px;display:inline-block;flex-grow:1">
